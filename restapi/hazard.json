        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "method",
            "in": "query",
            "description": "Method to get hazard amplification.",
            "required": true,
            "type": "string"
          },
          {
            "name": "datasetId",
            "in": "query",
            "description": "Dataset from data service.",
            "required": true,
            "type": "string"
          },
          {
            "name": "siteLat",
            "in": "query",
