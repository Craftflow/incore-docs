    "/earthquakes/soil/amplification": {
      "get": {
        "tags": [
          "earthquakes"
        ],
        "summary": "Returns earthquake site hazard amplification.",
        "description": " This returns the amplified hazard given a methodology (e.g. NEHRP), soil map dataset id (optional), latitude, longitude, ground shaking parameter (PGA, Sa, etc), hazard value, and default site class to use.",
