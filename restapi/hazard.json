            "required": true,
            "type": "number",
            "format": "double"
          },
          {
            "name": "siteLong",
            "in": "query",
            "description": "Longitude coordinate of the site.",
            "required": true,
            "type": "number",
            "format": "double"
          },
          {
            "name": "demandType",
            "in": "query",
            "description": "Liquefaction demand type. Ex: g.",
            "required": true,
            "type": "string"
          },
          {
            "name": "hazard",
            "in": "query",
            "description": "Hazard value.",
            "required": true,
            "type": "number",
            "format": "double"
          },
          {
            "name": "defaultSiteClass",
            "in": "query",
            "description": "Default site classification. Expected  A, B, C, D, E or F.",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
