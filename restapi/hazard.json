            "type": "string"
          },
          {
            "name": "earthquake-id",
            "in": "path",
            "description": "Earthquake dataset guid from data service.",
            "required": true,
            "type": "string"
          },
          {
            "name": "demandType",
            "in": "query",
            "description": "Liquefaction demand type. Ex: g.",
            "required": true,
